Array.__iter = func(self) {
    let iter = obj();
    iter.arr = self;
    iter.i = 0;
    iter.__next = func(self) {
        if self.i < self.arr.len() {
            let ret = self.arr.at(self.i);
            self.i = self.i + 1;
            ret
        } else {
            Iterator.end
        }
    };
    iter
};

let range = func(top) {
    let iter = obj();
    iter.i = 0;
    iter.top = top;
    iter.__iter = func(self) self;
    iter.__next = func(self) {
        if self.i < self.top {
            self.i = self.i + 1;
            self.i - 1
        } else {
            Iterator.end
        }
    };
    iter
};

let make_arr = func() {
    let arr = [];
    for i in range(10) {
        arr.push(i)
    }
    arr
};

Array.map = func(self, f) {
    let res = [];
    for elem in self {
        res.push(f(elem));
    }
    res
};

let arr = make_arr();
print(arr.map(func(x) x + 2), "\n");
